{
  "name": "@most/multicast",
  "version": "0.0.0",
  "description": "Standalone multicast for most.js",
  "main": "dist/multicast.js",
  "files": [
    "dist/multicast.js"
  ],
  "scripts": {
    "build-dist": "mkdir -p dist && rollup src/multicast.js | babel --presets es2015 --plugins transform-es2015-modules-umd --module-id '@most/multicast' -o dist/multicast.js",
    "build": "npm run build-dist && uglifyjs dist/multicast.js -o dist/multicast.min.js",
    "prepublish": "npm run build",
    "preversion": "npm run build",
    "unit-test": "mocha -r babel-register",
    "lint": "jscs -x src && jsinspect src && eslint src",
    "test": "npm run lint && npm run unit-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylors/most-multicast.git"
  },
  "keywords": [
    "most",
    "mostjs",
    "reactive",
    "event",
    "async",
    "streams",
    "multicast"
  ],
  "author": "Tylor Steinberer <tlsteinberger167@gmail.com> (github.com/TylorS)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tylors/most-multicast/issues"
  },
  "homepage": "https://github.com/tylors/most-multicast#readme",
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-plugin-transform-es2015-modules-umd": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^1.10.3",
    "jscs": "^2.9.0",
    "jsinspect": "^0.7.2",
    "mocha": "^2.4.5",
    "most": "^0.18.0",
    "rollup": "^0.25.2",
    "uglify-js": "^2.6.1"
  },
  "peerDependencies": {
    "most": "^0.18.0"
  }
}
